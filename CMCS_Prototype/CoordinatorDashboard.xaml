<Window x:Class="CMCS_Prototype.CoordinatorDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Coordinator Dashboard" Height="450" Width="800">
    <Grid Margin="10">
        <StackPanel>
            <TextBlock Text="Coordinator Dashboard" FontSize="20" FontWeight="Bold" Margin="0,0,0,20"/>

            <DataGrid AutoGenerateColumns="True" Height="200" Margin="0,0,0,10"/>
            <Button Content="Approve Claim" Width="200" Height="40" Margin="0,0,0,10"/>
            <Button Content="Reject Claim" Width="200" Height="40" Margin="0,0,0,20"/>

            <Button Content="Go to Manager Dashboard" Width="250" Height="40"/>
        </StackPanel>
    </Grid>
</Window>
